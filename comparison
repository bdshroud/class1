<?php
$x = 10;
$y = 12;

echo $x > $y; //greater than exucute if only true

echo $x < $y ; //less than exucute if only true

echo $x >= $y ; //greater than or equal execute if the terms equal or large 

echo $x <= $y ; //less than or equal execute if the terms equal or small 

echo $x == $y ; // only execute when both value are same

echo $x != $y ; // only execute when value are not smae;

?>
